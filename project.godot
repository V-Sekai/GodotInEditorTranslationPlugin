; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Localization"
run/main_scene="res://translation_demo/demo.tscn"
config/icon="res://project_icon.png"

[editor_plugins]

enabled=PoolStringArray( "res://addons/editor_icon_previewer/plugin.cfg", "res://addons/localization_editor_viewer/plugin.cfg" )

[global]

theme=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[locale]

translations=PoolStringArray( "res://translations/text.en.translation", "res://translations/text.fr.translation", "res://translations/item_descriptions/item_descriptions.en.translation", "res://translations/item_descriptions/item_descriptions.fr.translation", "res://translations/demo.en.translation", "res://translations/demo.fr.translation" )
locale_filter=[ 1, [  ] ]

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"

[viun_plugins]

translation_plugin/translations_location="res://translations/"
translation_plugin/valid_variable_names=PoolStringArray( "text" )
